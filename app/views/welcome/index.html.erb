<h2>Hello World</h2>
<p class="navbar2-text pull-right">
  <% if user_signed_in? %>
    Logged in as <strong><%= current_user.email %></strong>.
    <%= link_to 'Edit profile', edit_user_registration_path, :class => 'navbar2-link' %> |
    <%= link_to "Logout", destroy_user_session_path, method: :delete, :class => 'navbar2-link'  %>
  <% else %>
    <%= link_to "Sign up", new_user_registration_path, :class => 'navbar2-link'  %> |
    <%= link_to "Login", new_user_session_path, :class => 'navbar2-link'  %>
  <% end %>
</p>
<div id="navbar2" class="navbar2-collapse collapse">
  <ul class="nav2 navbar-nav">
    <li class="dropdown2">
      <a href="#" class="dropdown2-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Books <span class="caret"></span></a>
      <ul class="dropdown2-menu">
        <li><%= link_to "List", books_path %></li>
        <li><%= link_to "New", new_book_path %></li>
      </ul>
    </li>
  </ul>
</div>
<% if notice %>
  <p class="alert alert-success"><%= notice %></p>
<% end %>
<% if alert %>
  <p class="alert alert-danger"><%= alert %></p>
<% end %>
<%= yield %>
<p>
  The time is now: <%= Time.now %><head>
  </p>